{
  "name": "cy-intercept",
  "version": "1.0.0",
  "description": "Cypress intercept",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "report": "npm run mergeJsonReport && npm run generateReport",
    "mergeJsonReport": "npx mochawesome-merge ./mochawesome-report/*.json -o ./mochawesome-report/result.json ",
    "generateReport": "npx marge ./mochawesome-report/result.json -o ./mochawesome-report",
    "test:open": "npx cypress open",
    "test:headless": "npx cypress run --headless"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.15.1",
    "cypress-axe": "^1.1.0",
    "cypress-file-upload": "^5.0.8",
    "eslint-plugin-cypress": "^2.12.1"
  },
  "dependencies": {
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0"
  }
}
